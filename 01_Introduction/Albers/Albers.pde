/*
JSL
 simple sketch to show how to draw elements to the screen 
 */

void setup () {
  size (800, 800); //set the size of the sketch
  noStroke(); //turn off default stroke
  fill (11, 132, 151); // (red, green, blue)
  rect (0, 0, 800, 800); //( xstart, ystart, width, height)
  fill (75, 165, 72);
  rect (75, 125, 650, 650);
  fill (59, 119, 85);
  rect (150, 225, 500, 500);
  fill (63, 91, 102);
  rect (225, 325, 350, 350);
}
